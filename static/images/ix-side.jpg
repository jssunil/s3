<?xml version="1.0" encoding="UTF-8"?>
<svg width="400" height="200" viewBox="0 0 400 200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="carGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#00a86b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00754a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="windowGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#4682B4;stop-opacity:0.8" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="200" fill="#f8f9fa"/>
  
  <!-- Ground line -->
  <line x1="0" y1="180" x2="400" y2="180" stroke="#ddd" stroke-width="2"/>
  
  <!-- Car body -->
  <path d="M50,120 Q50,100 70,100 L330,100 Q350,100 350,120 L350,140 Q350,160 330,160 L320,160 Q320,180 300,180 Q280,180 280,160 L120,160 Q120,180 100,180 Q80,180 80,160 L70,160 Q50,160 50,140 Z" fill="url(#carGradient)" stroke="#00754a" stroke-width="2"/>
  
  <!-- Windows -->
  <path d="M80,100 Q80,80 100,80 L150,80 Q170,80 170,100 M230,80 Q250,80 250,100 L300,100 Q320,80 320,100" fill="url(#windowGradient)" stroke="#333" stroke-width="1"/>
  
  <!-- Wheels -->
  <circle cx='100' cy='160' r='15' fill="#2c3e50" stroke="#1a252f" stroke-width="2"/>
  <circle cx='300' cy='160' r='15' fill="#2c3e50" stroke="#1a252f" stroke-width="2"/>
  
  <!-- BMW kidney grille -->
  <ellipse cx="350" cy="127" rx="5" ry="8" fill="#333"/>
  <ellipse cx="350" cy="140" rx="5" ry="8" fill="#333"/>
  
  <!-- Headlights -->
  <ellipse cx="345" cy="120" rx="8" ry="4" fill="#fff" stroke="#ddd"/>
  <ellipse cx="345" cy="147" rx="8" ry="4" fill="#fff" stroke="#ddd"/>
  
  <!-- BMW logo area -->
  <circle cx="375" cy="133" r="12" fill="#0066cc" stroke="#fff" stroke-width="2"/>
  <text x="375" y="138" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-weight="bold" font-size="8">BMW</text>
  
  <!-- Model name -->
  <text x="200" y="170" text-anchor="middle" fill="#333" font-family="Arial, sans-serif" font-weight="bold" font-size="16">iX</text>
  
  <!-- Car type -->
  <text x="200" y="190" text-anchor="middle" fill="#666" font-family="Arial, sans-serif" font-size="12">SUV</text>
</svg>